{
  "name": "mhdobd/eloquent-relation-guard",
  "description": "A Laravel package for scanning model relations, And providing useful actions over them",
  "type": "library",
  "keywords": [
    "HasMany",
    "HasOne",
    "eloquent",
    "model",
    "forceCascadeDelete",
    "relations"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/MuhammadObadaa/eloquent-relation-guard/issues",
    "source": "https://github.com/MuhammadObadaa/eloquent-relation-guard"
  },
  "authors": [
    {
      "name": "Muhammad Obadaa Almasri",
      "email": "obadaa.masri@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": ">=6.0 <12.0"
  },
  "autoload": {
    "psr-4": {
      "EloquentRelation\\Guard\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EloquentRelation\\Guard\\EloquentRelationGuardServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}